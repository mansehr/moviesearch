{
  "manifest_version": 2,

  "name": "Filmsök",
  "description": "Sök efter streamingsbara filmer på svenska distributörers sidor",
  "version": "0.3.2",

  "icons": { "16": "icon.png",
           "48": "icon48.png",
          "128": "icon128.png" },
          
  "permissions": [
    "http://www.headweb.com/",
    "http://viaplay.se/",
    "http://api.voddler.com/",
    "http://sfanytime.com/",
    "https://www.film2home.se/",
    "http://www.swefilmer.com/",
	"http://www.dreamfilm.se/",
	"http://www.tfplay.org/",
	
    "http://mansehrmoviesearch.appspot.com/",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }

}
