{
  "manifest_version": 2,

  "name": "Filmsök",
  "description": "Sök efter streamingsbara filmer på svenska distributörers sidor",
  "version": "0.4.0",

  "icons": { "16": "icon.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "permissions" : [
  	"http://mansehrmoviesearch.appspot.com/",
    "storage"
  ],        
  "optional_permissions": [
	"http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }

}
