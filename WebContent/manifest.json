{
  "manifest_version": 2,

  "name": "Filmsök",
  "description": "Sök efter streamingsbara filmer på svenska distributörers sidor",
  "version": "0.3.7",

  "icons": { "16": "icon.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "permissions" : [
  	"http://mansehrmoviesearch.appspot.com/",
    "storage"
  ],        
  "optional_permissions": [
	"http://www.dreamfilmhd.com/",
    "https://www.film2home.se/",
    "http://www.headweb.com/",
	"https://www.plejmo.com/",
    "http://sfanytime.com/",
    "http://www.swefilmer.com/",
	"http://www.tfplay.org/",
    "http://viaplay.se/",
    "http://api.voddler.com/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }

}
